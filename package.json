{
  "name": "shieldbattery",
  "private": true,
  "version": "6.1.2",
  "description": "The modern way to play StarCraft: Brood War.",
  "homepage": "https://github.com/tec27/shieldbattery#readme",
  "main": "index.js",
  "engines": {
    "node": ">= 7.0.0"
  },
  "scripts": {
    "app": "cross-env SB_HOT=1 electron app",
    "autotest": "cross-env BABEL_ENV=node mocha --watch -R spec --compilers js:@babel/register --recursive --harmony test/",
    "build": "cross-env BABEL_ENV=node NODE_ENV=production webpack",
    "buildgame": "game/vcbuild.bat && cd game && yarn run build",
    "clean": "rimraf app/dist && rimraf app/styles",
    "dist": "yarn run clean && yarn run build && yarn run buildgame && build --win --ia32 --x64 -p always -c prod.yml",
    "diststaging": "yarn run clean && cross-env SB_SERVER=https\\://dev.shieldbattery.net yarn run build && yarn run buildgame && build --win --ia32 --x64 -p always -c staging.yml",
    "dev": "cross-env BABEL_ENV=node node -r @babel/register electron-dev-server.js",
    "lint": "eslint --ext .js,.jsx ./",
    "pack": "yarn run clean && yarn run build && yarn run buildgame && build --dir --win --ia32 --x64 -c prod.yml",
    "packstaging": "yarn run clean && cross-env SB_SERVER=https\\://dev.shieldbattery.net yarn run build && yarn run buildgame && build --dir --win --ia32 --x64 -c staging.yml",
    "postinstall": "cross-env-test SB_SERVER_ONLY=1 || electron-builder install-app-deps",
    "posttest": "yarn run lint",
    "test": "cross-env BABEL_ENV=node mocha -R spec --compilers js:@babel/register --recursive --harmony test/ && cd server && yarn test",
    "yarnall": "cd app && yarn && cd ../game && yarn && cd ../server && yarn && cd .. && yarn"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tec27/shieldbattery.git"
  },
  "keywords": [
    "brood war",
    "shieldbattery",
    "starcraft"
  ],
  "author": "Travis Collins <travis@tec27.com> (http://tec27.com/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tec27/shieldbattery/issues"
  },
